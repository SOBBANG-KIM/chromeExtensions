{
  "manifest_version": 3,
  "name": "Jira Quick Add",
  "version": "0.1.0",
  "description": "Create Jira issues using your existing Jira session.",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "Jira Quick Add",
    "default_popup": "popup.html"
  }
}
