{
  "manifest_version": 3,
  "name": "Jira Quick Add",
  "version": "0.1.2",
  "description": "Create Jira issues using your existing Jira session.",
  "permissions": [
    "storage",
    "activeTab",
    "identity"
  ],
  "host_permissions": [
    "<all_urls>",
    "https://www.googleapis.com/*"
  ],
  "oauth2": {
    "client_id": "1013698314494-ebkt0s9ht63snhhcpgevifj9s9dpmr4m.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "action": {
    "default_title": "Jira Quick Add",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/favicon_jira_16.png",
    "48": "images/favicon_jira_48.png",
    "128": "images/favicon_jira_128.png"
  }
}